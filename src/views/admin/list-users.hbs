{{> header_admin}}
<div class="flex items-center justify-between w-11/12 mx-auto pt-8">
    <h2>Lista usuarios</h2>
    <h2>Buscar</h2>
    <input type="text" placeholder="hola">
    <a class="flex items-center justify-center py-2 px-4 bg-pink-300 duration-300 hover:bg-pink-500 rounded" href="/admin">
        <i class="fas fa-arrow-left pr-2"></i><b>Ir atras</b>
    </a>
</div>
<section class="c-list-admin c-container">
    <table class="c-list-admin__table container__content">
        <thead>
            <tr>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>CÃ©dula</th>
                <th>Email</th>
                <th>Fecha de registro</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td scope="row">{{this.nombres}}</td>
                <td>{{this.apellidos}}</td>
                <td>{{this.dni}}</td>
                <td>{{this.email}}</td>
                <td>{{this.createdAt}}</td>
                <td><a href="/admin/make-seller/{{this.id}}" /><i class="fas fa-store"></i></a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</section>
