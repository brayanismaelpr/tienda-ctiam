<div class="m" id="sweet-alert">
    <div class="m__content" id="modal-sweet">
        <button onclick="deletemlogin()" class="m__cerrar"><i class="fas fa-times"></i></button>
        <div class="m-login">
            <form class="m-login__content" action="/login/signin" method="POST">
                <p class="m-login--p">Iniciar sesión</p>
                <div class="m-login__c-fist">
                    <div class="m-login__c__content">
                        <i class="fas fa-user"></i>
                        <input type="text" class="m-login__c--input" id="" placeholder="Correo eletrónico" name="email" required>
                    </div>
                </div>
                <div class="m-login__c-last">
                    <div class="m-login__c__content">
                        <i class="fas fa-lock"></i>
                        <input type="password" class="m-login__c--input" id="" placeholder="Contraseña" name="password" required>
                    </div>
                </div>
                <button type="submit" class="m-login--send">Iniciar sesión</button>
                <div class="m__btn-google"><a class="m__btn-google--a" href="/login/signin/google"><i
                            class="fab fa-google"></i>Entra con google</a></div>
                <div class="m-login__options">
                    <a class="m-login__options--a" href="">¿Olvidaste tu contraseña?</a>
                    <button type="button" onclick="registerModal()" class="m-login__options--a">Crear cuenta</button>
                </div>
            </form>
        </div>
    </div>
</div>