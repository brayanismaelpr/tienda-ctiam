<div class="m">
    <div class="m__content">
        <button onclick="deletemDeletecategoria()" class="m__cerrar"><i class="fas fa-times" aria-hidden="true"></i></button>
        <div class="m-login">
            <form class="m-login__content" action="/admin/frecuent-questions" method="POST" name="delete">
                <p class="m-login--p">Eliminar pregunta</p>
                <label class="m-login__c-fist m-login__c" style="margin-bottom: 20px">
                    <input type="text" class="m-login__c--input no-icon-input" id="question" placeholder="Pregunta" name="pregunta" required="">
